  <link name="base_link">
    <inertial>
      <mass value="5"/>
      <origin rpy="0 0 0" xyz="0.25 0.5 0.35"/>
      <inertia ixx="0.6208" ixy="0.0" ixz="0.0" iyy="0.30833" iyz="0.0" izz="0.5208"/>
    </inertial>

    <visual>
      <origin rpy="0 0 0" xyz="0.25 0.5 0.35"/>
      <geometry>
        <box size="0.5 1 0.7"/>
      </geometry>
    </visual>

  </link>
  
  <link name="link_1">
    <inertial>
      <mass value="1"/>
      <origin rpy="0 0 0" xyz="0.25 0.25 0.15"/>
      <inertia ixx="0.028333" ixy="0.0" ixz="0.0" iyy="0.028333" iyz="0.0" izz="0.04167"/>
    </inertial>

    <visual>
      <origin rpy="0 0 0" xyz="0.25 0.25 0.15"/>
      <geometry>
        <box size="0.5 0.5 0.3"/>
        <!-- <cylinder length="0.11" radius="0.5"/> -->
      </geometry>
    </visual>

  </link>

  <link name="link_2">
    <inertial>
      <mass value="1"/>
      <origin rpy="0 0 0" xyz="0.0 0.3 0.01"/>
      <inertia ixx="1" ixy="0.0" ixz="0.0" iyy="1" iyz="0.0" izz="1"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.3 0.01"/>
      <geometry>
        <box size="0.1 0.6 0.02"/>
      </geometry>
    </visual>
  </link>
  
  <link name="link_3">
    <inertial>
      <mass value="1"/>
      <origin rpy="0 0 0" xyz="0.0 0.01 0.2"/>
      <inertia ixx="1" ixy="0.0" ixz="0.0" iyy="1" iyz="0.0" izz="1"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.01 0.2"/>
      <geometry>
        <box size="0.1 0.02 0.4"/>
      </geometry>
    </visual>
    <!-- <inertial>
      <mass value="1"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.27083333333" ixy="0.0" ixz="0.0" iyy="0.27083333333" iyz="0.0" izz="0.27083333333"/>
